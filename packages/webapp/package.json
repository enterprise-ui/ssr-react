{
  "name": "@ssr-react/webapp",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "engines": {
    "node": ">= 10.14.2"
  },
  "main": "src/index.ts",
  "scripts": {
    "webapp:start": "yarn run dev",
    "dev": "yarn run build-server && npm-run-all --parallel dev:*",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "dev:server": "nodemon --watch public --exec \"node public/server.js\"",
    "prod": "yarn run build-server && npm-run-all --parallel prod:*",
    "prod:build-server": "webpack --config webpack.server.js --watch",
    "prod:build-client": "yarn run webpack-prod",
    "prod:server": "nodemon --watch public --exec \"node public/server.js\"",
    "build-server": "webpack --config webpack.server.js",
    "webpack-prod": "webpack -p --config webpack.client.prod.js"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "@ssr-react/core": "1.0.0",
    "@ssr-react/domain-example": "1.0.0",
    "axios": "0.19.2",
    "compression": "^1.7.4",
    "concurrently": "5.1.0",
    "express": "4.17.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "4.17.15",
    "nodemon": "2.0.2",
    "npm-run-all": "4.1.5",
    "prop-types": "^15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-lazy-load-image-component": "^1.4.1",
    "react-redux": "7.1.3",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "redux-thunk": "2.3.0",
    "serialize-javascript": "2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "7.3.3",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@types/compression": "^1.7.0",
    "@types/loadable__component": "^5.10.0",
    "@types/react-router-config": "^5.0.1",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "ts-loader": "^7.0.1",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "webpack": "5.0.0-beta.15",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "4.0.0-beta.8",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "4.2.2",
    "webpack-node-externals": "1.7.2"
  }
}
